{
  "name": "cloudlight-ai",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://cloudlight.ai",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "validate:jsonld": "node scripts/validate-jsonld.js",
    "validate:jsonld:strict": "node scripts/validate-jsonld.js --strict",
    "validate:hotel": "node scripts/validate-jsonld.js --file schemas/hotel.json --type Hotel",
    "validate:restaurant": "node scripts/validate-jsonld.js --file schemas/restaurant.json --type Restaurant",
    "validate:article": "node scripts/validate-jsonld.js --file schemas/article.json --type Article",
    "validate:website": "node scripts/validate-jsonld.js --file schemas/website.json --type WebSite",
    "validate:faq": "node scripts/validate-jsonld.js --file schemas/faq.json --type FAQ",
    "generate:schemas": "node scripts/generate-schemas.js",
    "test:jsonld": "npm test -- --testPathPattern=JsonLdTemplates"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
